{
  "name": "mtk-playground",
  "description": "mtk playground and doc",
  "private": true,
  "version": "0.0.0",
  "author": "sakitam-fdd <smilefdd@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite --host --mode development",
    "build": "rimraf dist && vite build --mode production",
    "preview": "vite preview --host",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint \"{src,mock,build}/**/*.{vue,js,jsx,mjs,ts,tsx}\" --fix --cache",
    "lint:prettier": "prettier --write \"src/**/*.{.vue,ts,html,css,less}\"",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*.{html,vue,css,scss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "commit": "git-cz",
    "semantic-release": "release-it"
  },
  "engines": {
    "node": ">= 18.16.0"
  },
  "packageManager": "pnpm@9.6.0",
  "keywords": [
    "vue3",
    "pinia",
    "vite",
    "typescript",
    "element-plus"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sakitam-fdd/mtk-playground.git"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@monaco-editor/loader": "^1.4.0",
    "@vueuse/core": "^10.10.0",
    "@vueuse/motion": "^2.2.3",
    "@vue/repl": "^4.3.1",
    "@vue/theme": "^2.2.12",
    "@shikijs/monaco": "^1.12.1",
    "axios": "^1.7.3",
    "axios-retry": "^4.5.0",
    "colord": "^2.9.3",
    "dayjs": "^1.11.11",
    "element-plus": "^2.7.8",
    "eventemitter3": "^5.0.1",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "nanoid": "^5.0.6",
    "nprogress": "^0.2.0",
    "shiki": "^1.12.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.12.1",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3",
    "vue-types": "^5.1.1",
    "vue3-lazyload": "^0.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@iconify/json": "^2.2.184",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@aurorafe/cz-config": "^1.0.1",
    "@aurorafe/eslint-config-base": "^1.1.0",
    "@aurorafe/eslint-config-vue": "^1.1.0",
    "@aurorafe/stylelint-config-vue": "^1.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.42",
    "@types/nprogress": "^0.2.3",
    "@unocss/preset-icons": "^0.59.3",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/compiler-sfc": "^3.4.37",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "less-vars-to-js": "^1.3.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "release-it": "^17.6.0",
    "sass": "^1.71.0",
    "stylelint": "^16.2.1",
    "sucrase": "^3.35.0",
    "typescript": "^5.3.3",
    "unocss": "^0.62.1",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.19.2",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.11.5",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-fake-server": "^2.1.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imp": "^2.4.0",
    "vitest": "^2.0.5",
    "vue-eslint-parser": "^9.4.3",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
