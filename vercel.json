{
  "github": {
    "silent": true
  },
  "rewrites": [{ "source": "/:path*", "destination": "/index.html" }],
  "redirects": [
    {
      "source": "/mtk-playground/static/:path*",
      "destination": "/static/:path*",
      "permanent": true
    },
    {
      "source": "/mtk-playground/resources/:path*",
      "destination": "/resources/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/thumbnail/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
